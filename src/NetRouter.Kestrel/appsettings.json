{
  "netRouter": {
    "enableRewindRequest": false,
    "enableRewindResponse": false
  },

  "routingFilter": {
    "certificates": {
      //"alias": {
      //}
    },

    "filters": {
      "console1": { "type": "NetRouter.Kestrel.Filters.ConsoleFilter, NetRouter.Kestrel" }
    },

    "mappings": {
      "test1": {
        "path": "/localhost",
        "strategy": "stripPath", //none|stripPath|replace
        "httpCLient": {
          "sslProtocol": "none"
        },
        "targetHosts": [ "127.0.0.1:80", "localhost" ]
        //  "filters": []
      },

      "testPage": {
        "path": "/testpage",
        "strategy": "stripPath",
        "httpCLient": {
          "sslProtocol": ""
        },
        "protocol": "https",
        //"targetHosts": [ "www.webscantest.com" ],
        "targetHosts": [ "www.ssllabs.com" ],
        "filters": [ "console1" ]
      }
    }
  }
}